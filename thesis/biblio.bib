    journal = {Revista de saude publica},
    keywords = {80 and over,Adolescent,Adult,Age Distribution,Aged,Brazil/epidemiology,Cause of Death,Female,Humans,Male,Middle Aged,Sex Distribution,Suicide/ statistics {\&} numerical data/trends},
