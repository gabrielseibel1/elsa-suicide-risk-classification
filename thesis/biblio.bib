    issn = {15164446},
    journal = {Revista Brasileira de Psiquiatria},
    keywords = {Anxiety,Common mental disorders,Depression,Low and middle-income countries,Mental health},
    number = {2},
    pages = {91--97},
    pmid = {27304755},
    title = {{Common mental disorders and sociodemographic characteristics: Baseline findings of the Brazilian Longitudinal Study of Adult Health (ELSA-Brasil)}},
