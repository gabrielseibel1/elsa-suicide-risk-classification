    file = {:Users/i508316/Library/Application Support/Mendeley Desktop/Downloaded/Barros et al. - 2017 - Suicide detection in Chile Proposing a predictive model for suicide risk in a clinical sample of patients with mo.pdf:pdf},
    issn = {15164446},
    journal = {Revista Brasileira de Psiquiatria},
    keywords = {Data mining,Mood disorders,Suicide},
    number = {1},
    pages = {1--11},
