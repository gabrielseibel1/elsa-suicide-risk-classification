    file = {:Users/i508316/Library/Application Support/Mendeley Desktop/Downloaded/Meltzer et al. - 2012 - Physical ill health, disability, dependence and depression Results from the 2007 national survey of psychiatric.pdf:pdf},
    issn = {19366574},
    keywords = {Dependence,Depression,Disability,Physical ill health},
    number = {2},
